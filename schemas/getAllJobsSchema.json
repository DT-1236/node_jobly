{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [],
  "properties": {
    "search": {
      "$id": "#/properties/search",
      "type": "string",
      "title": "The Search Schema",
      "default": "",
      "examples": ["Cake Taster"],
      "pattern": "^(.*)$"
    },
    "min_salary": {
      "$id": "#/properties/min_salary",
      "type": "integer",
      "title": "The Min_salary Schema",
      "default": 0,
      "minimum": 0,
      "examples": [40000]
    },
    "min_equity": {
      "$id": "#/properties/min_equity",
      "type": "number",
      "title": "The Min_equity Schema",
      "default": 0,
      "minimum": 0,
      "maximum": 1,
      "examples": [0.75]
    },
    "company_handle": {
      "$id": "#/properties/min_equity",
      "type": "string",
      "title": "The Company_handle Schema",
      "examples": ["cake"]
    }
  }
}
